{
  "version": 2,
  "buildCommand": "npx prisma generate",
  "routes": [
        {
            "src": "/(.*)",
            "dest": "/app2.js",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]

}
